{
  "builds": [
    {
      "src": "src/pandora_cloud/server.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source":"/api/:path((?!auth|accounts).*)",
      "destination":"https://oss.keyplusys.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "src/pandora_cloud/server.py"
    }
  ]
}
